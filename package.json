{
  "name": "documentify",
  "description": "Modular HTML bundler",
  "repository": "stackhtml/documentify",
  "version": "2.0.2",
  "bin": "./bin.js",
  "scripts": {
    "deps": "dependency-check . && dependency-check . --extra --no-dev",
    "start": "node .",
    "test": "standard && npm run deps && ./bin.js ./example"
  },
  "dependencies": {
